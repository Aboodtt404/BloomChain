          }
        },
        {
          "match": "**/*.txt",
          "headers": {
            "Content-Type": "text/plain"
          }
        },
        {
          "match": "**/*.mp3",
          "headers": {
            "Content-Type": "audio/mpeg"
          }
        },
        {
          "match": "**/*.ogg",
          "headers": {
            "Content-Type": "audio/ogg"
          }
